generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Habit {
  id        Int         @id @default(autoincrement())
  name      String
  HabitData HabitData[]
}

model HabitData {
  id        Int      @id @default(autoincrement())
  habit     Habit    @relation(fields: [habitId], references: [id])
  habitId   Int
  date      DateTime
  completed Boolean
}
